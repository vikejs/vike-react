  },
  "devDependencies": {
    "@brillout/release-me": "^0.1.14",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vike": "^0.4.156"
  },
  "dependencies": {
    "react-streaming": "^0.3.19"
  },
  "typesVersions": {
    "*": {
