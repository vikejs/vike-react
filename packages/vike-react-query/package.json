  },
  "devDependencies": {
    "@brillout/release-me": "^0.3.8",
    "@tanstack/react-query": "^5.20.1",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "jsdom": "^24.0.0",
    "react": "^18.3.1",
    "react-streaming": "^0.3.43",
    "rimraf": "^5.0.5",
    "typescript": "^5.5.3",
    "vike": "^0.4.203",
    "vike-react": "^0.5.7",
    "vite": "^5.4.0",
    "vitest": "^1.2.2"
