      "usePageContext": [
        "./dist/components/usePageContext.d.ts"
      ],
      "useData": [
        "./dist/components/useData.d.ts"
      ],
      "renderer/onRenderHtml": [
        "./dist/renderer/onRenderHtml.d.ts"
      ],
      "renderer/onRenderClient": [
        "./dist/renderer/onRenderClient.d.ts"
      ]
